{
    "name"             : "laravel/laravel",
    "description"      : "The Laravel Framework.",
    "keywords"         : [
        "framework",
        "laravel"
    ],
    "license"          : "MIT",
    "type"             : "project",
    "require"          : {
        "laravel/framework"          : "4.2.*",
        "barryvdh/laravel-ide-helper": "1.*",
        "barryvdh/laravel-cors"      : "0.2.*"
    },
    "autoload"         : {
        "classmap": [
            "app/commands",
            "app/controllers",
            "app/models",
            "app/database/migrations",
            "app/database/seeds",
            "app/tests/TestCase.php",
            "app/Arato"
        ],
        "files"   : [
            "vendor/pirminis/maybe-monad/lib/global.php"
        ]
    },
    "scripts"          : {
        "post-install-cmd"       : [
            "$_ artisan clear-compiled",
            "$_ artisan optimize"
        ],
        "pre-update-cmd"         : [
            "$_ artisan clear-compiled"
        ],
        "post-update-cmd"        : [
            "$_ artisan clear-compiled",
            "$_ artisan ide-helper:generate",
            "$_ artisan optimize"
        ],
        "post-create-project-cmd": [
            "$_ artisan key:generate"
        ]
    },
    "config"           : {
        "preferred-install": "dist"
    },
    "minimum-stability": "stable",
    "require-dev"      : {
        "fzaninotto/faker"         : "1.4",
        "anahkiasen/underscore-php": "dev-master",
        "pirminis/maybe-monad"     : "dev-master"
    }
}
